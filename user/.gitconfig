[apply]
  whitespace = fix
[branch]
  autoSetupRebase = always
  sort = refname
[checkout]
  defaultRemote = origin
  guess = true
  workers = -1 # use number of logical cores available
[clean]
  requireForce = true
[clone]
  defaultRemoteName = origin
[color]
  advice = auto
  branch = auto
  diff = auto
  grep = auto
  interactive = auto
  push = auto
  remote = auto
  showBranch = auto
  status = auto
  transport = auto
  ui = auto
[column]
  ui = never
[commit]
  cleanup = strip
[core]
  autocrlf = input
  eol = lf
  filemode = false
  hideDotFiles = dotGitOnly
  logAllRefUpdates = true
  safecrlf = false
  warnAmbiguousRefs = true
  whitespace = tab-in-indent,tabwidth=2,trailing-space
[diff]
  relative = true
[fetch]
  prune = true
  pruneTags = true
  showForcedUpdates = true
  writeCommitGraph = true
[help]
  autoCorrect = prompt
[index]
  threads = true
[init]
  defaultBranch = main
[pull]
  rebase = true
[push]
  autoSetupRemote = true
  default = simple
  useForceIfIncludes = true
[rebase]
  autoSquash = true
  autoStash = true
  missingCommitsCheck = error
  stat = true
  updateRefs = true
[rerere]
  enabled = true
[reset]
  quiet = false
[status]
  aheadBehind = true
  branch = true
  relativePaths = false
  showStash = true
  showUntrackedFiles = all
[stash]
  showIncludeUntracked = true
  showPatch = true
  showStat = true
[svn]
  rmdir = true
[svn-remote "svn"]
  automkdirs = false
[user]
  email = 5903869+herberttn@users.noreply.github.com
  name = herberttn
  useConfigOnly = true
